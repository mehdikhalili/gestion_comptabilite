<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block navbar %}
            {{ include('partials/_navbar.html.twig') }}
        {% endblock %}
        
        <main role="main" class="container">
            {% block body %}{% endblock %}
        </main>
    </body>
</html>
